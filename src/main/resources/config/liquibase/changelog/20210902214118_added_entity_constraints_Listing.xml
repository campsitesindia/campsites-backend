        <addForeignKeyConstraint baseColumnNames="feature_id"
                                 baseTableName="listing"
                                 constraintName="fk_listing__feature_id"
                                 referencedColumnNames="id"
                                 referencedTableName="features"/>

        <addForeignKeyConstraint baseColumnNames="room_id"
                                 baseTableName="listing"
                                 constraintName="fk_listing__room_id"
                                 referencedColumnNames="id"
                                 referencedTableName="room"/>

        <addForeignKeyConstraint baseColumnNames="owner_id"
                                 baseTableName="listing"
                                 constraintName="fk_listing__owner_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>
